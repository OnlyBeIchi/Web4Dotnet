        <td>
            @Html.DisplayFor(modelItem => item.Khoan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NDKhoan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MucPhatduoi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MucPhattren)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.QAQC)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Hiển thị", "Details", new { id=item.ID }) |
            @Html.ActionLink("Xóa", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
