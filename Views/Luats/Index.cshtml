        </th>
        <th>
            @Html.DisplayNameFor(model => model.MucPhatduoi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MucPhattren)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.QAQC)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
